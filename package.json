{
  "name": "ts-demo",
  "version": "1.0.0",
  "description": "",
  "main": "demo.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha -r ./node_modules/ts-node/register --watch test/**/*.js --colors",
    "dev": "./node_modules/.bin/ts-node-dev --respawn --transpileOnly src/index.ts",
    "start-node": "yarn build:live",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:tsc": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build:start": "nodemon --watch 'scripts/**/*.js' --exec 'node' scripts/start.js"
  },
